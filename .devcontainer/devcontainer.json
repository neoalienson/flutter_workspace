{
  "name": "Flutter Web App Development",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": true,
      "installOhMyZsh": true,
      "upgradePackages": true
    }
  },
  "forwardPorts": [8080],
  "postCreateCommand": "flutter precache",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.flutter",
        "Dart-Code.dart-code",
        "esbenp.prettier-vscode",
        "saoudrizwan.claude-dev"
      ],
      "settings": {
        "your.cline.model.setting.key": "your-model-name"
      }
    }
  }
}